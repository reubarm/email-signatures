{
  "name": "@fairfx/geometry-web",
  "repository": "https://github.com/EqualsGroup/geometry-web.git",
  "version": "3.43.1",
  "description": "Equals design system",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --maxWorkers=2",
    "test:ts": "tsc --noEmit",
    "lint": "eslint './**/*.{tsx,ts}'",
    "prepack": "yarn build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o public",
    "lint:fix": "yarn lint --fix",
    "build": "tsc",
    "chromatic": "CHROMATIC_APP_CODE=yhbdw6rwuyi chromatic test --exit-zero-on-changes",
    "semantic-release": "semantic-release",
    "now-build": "yarn && yarn build-storybook",
    "now-start": "start-storybook",
    "release": "HUSKY_SKIP_HOOKS=1 yarn semantic-release",
    "feature:publish": "yarn ci-pilot publish feature"
  },
  "optionalDependencies": {
    "@fairfx/wc-configs": "^8.1.11"
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@emotion/is-prop-valid": "^0.8.8",
    "@emotion/styled": "^10.0.27",
    "@fairfx/geometry-icons": "^1.1.0",
    "@fairfx/geometry-palette": "^0.1.0",
    "@mdx-js/react": "^1.5.3",
    "emotion-theming": "^10.0.27",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "polished": "^3.4.4",
    "ramda": "^0.27.0",
    "react-dates": "^21.8.0",
    "react-phone-input-2": "^2.13.9",
    "react-select": "^3.1.0",
    "react-tether": "^2.0.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.8.3",
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/prompt": "^8.3.4",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.4",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@storybook/addon-a11y": "^5.3.18",
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-docs": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.13",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.13",
    "@storybook/cli": "^5.3.13",
    "@storybook/react": "^5.3.13",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^10.0.0",
    "@types/jest": "^24.0.25",
    "@types/ramda": "^0.26.41",
    "@types/react": "^16.9.17",
    "@types/react-dates": "^17.1.10",
    "@types/react-dom": "^16.9.4",
    "@types/react-select": "^3.0.12",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.1.1",
    "ci-pilot": "^1.1.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-junit": "^5.2.0",
    "jsx-to-string-loader": "^1.2.0",
    "path": "^0.12.7",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-is": "^16.13.1",
    "react-test-renderer": "^16.12.0",
    "react-view": "^2.1.0",
    "semantic-release": "^16.0.1",
    "storybook-chromatic": "^3.4.1",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "tsc && yarn lint && yarn test --coverage"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "files": [
    "dist",
    "README.md"
  ]
}
