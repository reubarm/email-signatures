      windowSelection.addRange(range);\n    }\n    try {\n      let successful = document.execCommand(\"copy\");\n      setState((prevState) => ({\n        ...prevState,\n        copied: true,\n      }));\n    } catch (err) {}\n  };\n\n  const signatureData = () => {\n    return (\n      <>\n        <Signature\n          name={state.name}\n          jobrole={state.jobrole}\n          landline={state.landline}\n          mobile={state.mobile}\n          linkedin={state.linkedin}\n          spectrum={state.spectrum}\n          equals={state.equals}\n        />\n      </>\n    );\n  };\n\n  const EmailSignature = () => (\n    <div>\n      <Container>\n        <Text as=\"h4\">Your email signature:</Text>\n        <Component signature={true}>\n          <Row>\n            <Column>{signatureData()}</Column>\n          </Row>\n        </Component>\n        <ButtonWrapper>\n          <PrimaryButton onClick={copySignature}>\n            {state.copied ? \"Copied\" : \"Copy Signature\"}\n          </PrimaryButton>\n        </ButtonWrapper>\n        <Instructions />\n      </Container>\n    </div>\n  );\n\n  return showEmailSignature ? (\n    <EmailSignature />\n  ) : (\n    <Container>\n      <Text as=\"h3\">Create your email signature</Text>\n      <Text fontSize=\"large\" fontWeight=\"bold\">\n        Select your company:\n      </Text>\n\n      <CompanySelect>\n        <Companies>\n          <Brand onClick={equalsSelect} className={equalsImg}>\n            <img src={EqualsLogo} />\n          </Brand>\n          <Brand onClick={spectrumSelect} className={spectrumImg}>\n            <img src={SpectrumLogo} />\n          </Brand>\n        </Companies>\n      </CompanySelect>\n\n      <Text as=\"h4\">Enter your details:</Text>\n\n      <Component signature={false}>\n        <form noValidate autoComplete=\"off\">\n          <Row>\n            <Column>\n              <InputField>\n                <Input\n                  placeholder=\"Full Name\"\n                  isReadonly={fa